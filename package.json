    "socket.io": "^4.4.1",
    "twilio": "^3.73.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
